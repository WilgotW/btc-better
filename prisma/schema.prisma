generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model users {
  id       Int      @id @default(autoincrement())
  username String @db.VarChar(80)
  email    String @db.VarChar(80)
  password String @db.VarChar(80)
  balance  Float
}
model bets {
  id     Int      @id @default(autoincrement())
  ticker    String
  userid    Int
  startdate  Int
  enddate    Int
  duration   Int
  amount Int
  startvalue Float
  done Boolean @default(false)
}